{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AWS": {
    "Profile": "serverless"
  },
  "ApiKeyValidationSettings": {
    "IsEnabled": false,
    "ApiKey": "1234567",
    "WhiteList": [
      "/swagger"
    ],
    "HeaderName": "x-api-key"
  },
  "EventBusSettings": {
    "IsEnabled": true,
    "TopicArn": "XX"
  },
  "EmailSettings": {
    "IsEnabled": true,
    "Provider": "XX",
    "From": "XX",
    "FromName": "XX"
  },
  "SmsSettings": {
    "IsEnabled": true,
    "Provider": "XX"
  },
  "OtpSettings": {
    "IsTestMode": true,
    "TestCode": "11111"
  },
  "UniqueKeySettings": {
    "EmailShouldBeUnique": true,
    "PhoneShouldBeUnique": true,
    "UserNameShouldBeUnique": true
  },
  "UserVerificationSettings": {
    "EmailShouldVerifyOnRegister": true,
    "PhoneShouldVerifyOnRegister": true,
    "ExpireInXMinute": 180
  },
  "AllowedHosts": "*"
}
